  buffer << ", " << DisasmRegister(rt);
};

vmsr.execute = {
  if (not CheckCondition(cpu, cond)) return;
  
  switch (spr) {
  default: throw 0;
  case 1: {
    cpu.FPSCR().Set( ALL32, cpu.GetGPR( rt ) );
  } break;
  }
};

