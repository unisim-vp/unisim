    cpu.MemWrite32( address + 0, cpu.GetVU64( 2*(vd+reg) + 0 ) );
    cpu.MemWrite32( address + 4, cpu.GetVU64( 2*(vd+reg) + 1 ) );
    address += 8;
  }
};

