  DisasmMLSImmOffset_offset(u, rn, immedH, immedL, buffer);
}

ldrh_imm_pre.disasm = {
  buffer << "ldr";
  buffer << DisasmCondition(cond);
  buffer << "h r" << (unsigned int) rd << ", ";
  // MLS = Miscellaneous Load Store
