}

ldrh_reg_post.disasm = {
  buffer << "ldr";
  buffer << DisasmCondition(cond);
  buffer << "h r" << (unsigned int) rd << ", ";
  DisasmMLSReg_post(u, rn, rm, buffer);
}

ldrh_reg_offset.disasm = {
  buffer << "ldr";
  buffer << DisasmCondition(cond);
  buffer << "h r" << (unsigned int) rd << ", ";
  DisasmMLSReg_offset(u, rn, rm, buffer);
}

ldrh_reg_pre.disasm = {
  buffer << "ldr";
  buffer << DisasmCondition(cond);
  buffer << "h r" << (unsigned int) rd << ", ";
