}

ldrh_imm_post.execute = {
	typedef Operation<STATE> inherited;

	uint32_t address;
	uint32_t rn_value = cpu.GetGPR(rn);


	if(not CheckCondition(cpu, cond)) return;
	
	address = rn_value; // address used is the contents of rn
	// TODO: Check Alignment Exception (address & ~(uint32_t)(0x02))
	cpu.SetGPR(rn, MLSImmOffset(u, rn_value, immedH, immedL)); // update rn
	cpu.SetGPR( rd, cpu.MemRead16( address & -2 ) );
}

ldrh_imm_offset.execute = {
	typedef Operation<STATE> inherited;

	uint32_t address;
	uint32_t rn_value = cpu.GetGPR(rn);


